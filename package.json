{
  "name": "fiona",
  "version": "2.2.2",
  "description": "tiny yet powerful pseudo random data generator",
  "repository": "billymoon/fiona",
  "main": "fiona.js",
  "scripts": {
    "build": "webpack -o fiona.js",
    "build:core": "webpack-cli --config webpack.config.core.js",
    "docs": "next",
    "docs:build": "next build && next export -o docs",
    "docs:deploy": "cd docs && now",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:acceptance": "mkdir -p docs/reports/cucumber; cucumber-js -f json:docs/reports/cucumber/report.json -f progress && node scripts/cucumber-html-report"
  },
  "author": "Billy Moon",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-styled-components": "^1.2.0",
    "bezier-easing": "^2.0.3",
    "cucumber": "^4.2.1",
    "cucumber-html-reporter": "^4.0.2",
    "fetch-pretender": "^1.5.0",
    "freactal": "^2.0.3",
    "jest": "^22.4.4",
    "lodash": "^4.17.5",
    "next": "^6.0.2",
    "randexp": "^0.4.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^3.2.6",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "docs/reports/coverage"
  }
}
