{
  "name": "fiona",
  "version": "3.0.0",
  "description": "tiny yet powerful pseudo random data generator",
  "repository": "billymoon/fiona",
  "main": "fiona.min.js",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "test": "jest",
    "size-limit": "size-limit",
    "build": "taskr build",
    "deploy": "taskr deploy",
    "fix": "taskr fix",
    "git-pre-commit": "taskr precommit",
    "git-post-commit": "taskr postcommit",
    "git-post-merge": "taskr postcommit"
  },
  "author": "Billy Moon",
  "license": "ISC",
  "devDependencies": {
    "@taskr/clear": "^1.1.0",
    "@taskr/esnext": "^1.1.0",
    "@weco/next-plugin-transpile-modules": "0.0.2",
    "@zeit/git-hooks": "^0.1.4",
    "bezier-easing": "^2.0.3",
    "fetch-pretender": "^1.5.0",
    "git-tools": "^0.3.0",
    "jest": "^22.4.4",
    "jsx-components": "1.2.0",
    "lodash.template": "^4.4.0",
    "next": "^7.0.2",
    "randexp": "^0.4.6",
    "react": "^16.4.1",
    "react-dom": "^16.0.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "size-limit": "^0.21.0",
    "standard": "^11.0.1",
    "taskr": "^1.1.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.docs.js",
      "!src/docs/**"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "misc"
    ]
  },
  "dependencies": {
    "react-highlight": "^0.12.0"
  },
  "size-limit": [
    {
      "config": "webpack.config.core.js",
      "limit": "5 KB"
    },
    {
      "config": "webpack.config.js",
      "limit": "20 KB"
    },
    {
      "path": "fiona.min.js",
      "limit": "10 KB"
    },
    {
      "path": "fiona.core.min.js",
      "limit": "2 KB"
    }
  ]
}
