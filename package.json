{
  "name": "fiona",
  "version": "2.2.2",
  "description": "tiny yet powerful pseudo random data generator",
  "repository": "billymoon/fiona",
  "main": "fiona.js",
  "scripts": {
    "start": "next start",
    "dev": "next",
    "test": "jest",
    "build": "run-p build:** && run-s report:**",
    "build:docs": "next build && fs.clean static/reports",
    "build:fiona": "webpack",
    "build:fiona-core": "webpack --config webpack.config.core.js",
    "report:coverage": "jest --coverage && mv coverage/lcov-report static/reports/coverage",
    "deploy": "DEPLOY_URI=$(now) && now alias $DEPLOY_URI https://fiona.now.sh"
  },
  "author": "Billy Moon",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-styled-components": "^1.2.0",
    "bezier-easing": "^2.0.3",
    "fetch-pretender": "^1.5.0",
    "freactal": "^2.0.3",
    "fs-dot": "^4.0.2",
    "jest": "^22.4.4",
    "lodash": "^4.17.5",
    "next": "^6.0.2",
    "npm-run-all": "^4.1.3",
    "randexp": "^0.4.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^3.2.6",
    "webpack": "^4.8.3",
    "webpack-command": "^0.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.docs.js",
      "!src/docs/**"
    ]
  },
  "dependencies": {}
}
