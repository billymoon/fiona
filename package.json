{
  "name": "fiona",
  "version": "3.4.2",
  "description": "tiny yet powerful pseudo random data generator",
  "repository": "billymoon/fiona",
  "main": "static/fiona.min.js",
  "scripts": {
    "start": "node scripts/dev",
    "dev": "node scripts/dev",
    "coverage": "deno test src --coverage=coverage && deno coverage coverage --lcov > coverage/coverage.lcov && genhtml -o coverage/html coverage/coverage.lcov",
    "test": "deno test src",
    "build": "node scripts/build",
    "fix": "node scripts/fix",
    "git-pre-commit": "node scripts/precommit"
  },
  "author": "Billy Moon",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@types/react": "^16.9.1",
    "@zeit/git-hooks": "^0.1.4",
    "babel-loader": "^8.0.6",
    "bezier-easing": "^2.1.0",
    "dependency-cruiser": "^4.6.3",
    "esm": "^3.2.25",
    "fs-extra": "^7.0.1",
    "lodash.template": "^4.4.0",
    "next": "^11.0.1",
    "prettier": "^1.15.2",
    "randexp": "^0.4.6",
    "react": "^16.4.1",
    "react-dom": "^16.0.0",
    "react-highlight": "^0.12.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "size-limit": "^0.21.0",
    "typescript": "^3.5.3",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "size-limit": [
    {
      "config": "webpack.config.core.js",
      "limit": "100 KB"
    },
    {
      "config": "webpack.config.js",
      "limit": "100 KB"
    },
    {
      "path": "static/fiona.min.js",
      "limit": "20 KB"
    },
    {
      "path": "static/fiona.core.min.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "fetch-mock": "^7.5.1",
    "json-graphql-server": "^2.1.3",
    "pretender": "^3.0.2"
  }
}
