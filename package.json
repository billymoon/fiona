{
  "name": "fiona",
  "version": "1.4.0",
  "description": "tiny yet powerful pseudo random data generator",
  "repository": "billymoon/fiona",
  "main": "fiona.js",
  "scripts": {
    "demo": "node -r @std/esm demo",
    "build": "npm run build:core && npm run build:full",
    "build:full": "webpack",
    "build:core": "webpack --config webpack.config.core.js",
    "docs": "next",
    "docs:build": "next build && next export -o docs",
    "pretest": "npm run build",
    "test": "ava",
    "test:coverage": "npm run pretest && nyc ava"
  },
  "author": "Billy Moon",
  "license": "ISC",
  "@std/esm": {
    "esm": "all"
  },
  "devDependencies": {
    "@std/esm": "^0.7.1",
    "ava": "^0.22.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-styled-components": "^1.2.0",
    "fetch-pretender": "^1.5.0",
    "isomorphic-fetch": "^2.2.1",
    "mersenne-twister": "^1.1.0",
    "next": "^3.2.2",
    "nyc": "^11.2.1",
    "randexp": "^0.4.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rollup": "^0.50.0",
    "styled-components": "^2.1.2",
    "webpack": "^3.6.0"
  }
}
